{
  "name": "gh-pages",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ../../public/index.html ../../public/assets",
    "prebuild": "npm run clean",
    "prebuild:windows": "npm run clean",
    "build": "NODE_ENV=production reacthtmlpack buildToDir ../../public './views/**/*.html.js'",
    "build:windows": "set NODE_ENV=production&& reacthtmlpack buildToDir ../../public './views/**/*.html.js'",
    "predev": "npm run clean",
    "dev": "reacthtmlpack buildToDir ../../public './views/**/*.html.js'",
    "prestart": "npm run clean",
    "prestart:windows": "npm run clean",
    "prestart:docker": "npm run clean",
    "start": "reacthtmlpack devServer ./Client.webpackConfig.js ../../public './views/**/*.html.js'",
    "start:windows": "reacthtmlpack devServer ./Client.webpackConfig.js ../../public './views/**/*.html.js'"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "reacthtmlpack": "^1.2.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "animate.css": "^3.4.0",
    "bootstrap-sass": "^3.3.5",
    "can-use-dom": "^0.1.0",
    "classnames": "^2.1.5",
    "fbjs": "^0.3.1",
    "isomorphic-fetch": "^2.2.0",
    "node-libs-browser": "^0.5.3",
    "prismjs": "git+https://github.com/PrismJS/prism.git#gh-pages",
    "raf": "^3.1.0",
    "react": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-github-fork-ribbon": "^0.4.2",
    "react-google-maps": "^4.4.0",
    "react-icons": "^0.1.4",
    "react-prism": "^3.0.0",
    "react-pure-render": "^1.0.2",
    "react-toastr": "^2.1.0",
    "toastr": "^2.1.2"
  }
}
