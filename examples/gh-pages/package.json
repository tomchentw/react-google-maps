{
  "name": "gh-pages",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ../../public/index.html ../../public/assets ../../public/tmp",
    "prebuild": "npm run clean",
    "prebuild:windows": "npm run clean",
    "build": "NODE_ENV=production reacthtmlpack buildToDir ../../public ./views/*.html.js",
    "build:windows": "set NODE_ENV=production&& reacthtmlpack buildToDir ../../public ./views/*.html.js",
    "dev": "npm run clean && reacthtmlpack buildToDir ../../public ./views/*.html.js",
    "prestart": "npm run clean",
    "prestart:windows": "npm run clean",
    "prestart:docker": "npm run clean",
    "start": "reacthtmlpack devServer ./Client.webpackConfig.js ../../public ./views/*.html.js",
    "start:windows": "reacthtmlpack devServer ./Client.webpackConfig.js ../../public ./views/*.html.js",
    "start:docker": "reacthtmlpack devServer ./Client.webpackConfig.js ../../public ./views/*.html.js --docker"
  },
  "devDependencies": {
    "babel-core": "^5.3.3",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.12.0",
    "extract-text-webpack-plugin": "^0.8.0",
    "file-loader": "^0.8.1",
    "node-sass": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.7",
    "reacthtmlpack": "^0.3.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^1.0.1",
    "style-loader": "^0.12.2",
    "webpack": "^1.9.5",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "animate.css": "^3.2.5",
    "bootstrap-sass": "^3.3.4",
    "classnames": "^2.1.1",
    "node-libs-browser": "^0.5.2",
    "prismjs": "git+https://github.com/PrismJS/prism.git#master",
    "raf": "^3.1.0",
    "react": "^0.13.3",
    "react-github-fork-ribbon": "^0.4.1",
    "react-prism": "^1.3.0",
    "react-toastr": "^1.3.0",
    "toastr": "^2.0.4"
  }
}
