    "html-webpack-plugin": "^1.1.0",
    "jest-cli": "^0.1.18",
    "jquery": "^2.1.1",
    "jsx-loader": "^0.12.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^0.5.0",
    "react-prism": "^1.0.0",
    "react-toastr": "^1.1.2",
    "react-tools": "^0.12.0",
    "sass-loader": "^0.6.0",
    "style-loader": "^0.8.1",
    "tomchentw-npm-dev": "^1.1.0",
    "webpack": "^1.4.7",
    "webpack-dev-server": "^1.6.5"
  },
  "dependencies": {
    "deep-equal": "^0.2.1",
    "object-path": "^0.9.2",
    "react": "^0.12.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/test/__preprocessor__.js",
    "testPathDirs": [
      "<rootDir>/src"
